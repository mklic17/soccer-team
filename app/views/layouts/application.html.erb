<!DOCTYPE html>
<html>
  <head>
    <title>Soccer</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <nav class="navbar navbar-default">
    <div class="container-fluid">

      <div class="navbar-header">
        <h2><%= link_to"Soccer Teams", :controller=>"static", :action=>"home", :class=>"navbar-brand" %></h2>
      </div>

      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav navbar-right">
          <li class="active">  <%= link_to"Table", :controller=>"static", :action=>"main", :class=>"" %></li>
          <li class="active">  <%= link_to"Team Sign Up", :controller=>"static", :action=>"signUp", :class=>"" %></li>
        </ul>
      </div>
    </nav>

    <%= yield %>
  </body>
</html>
